  <%= profileName %>:
    <<: *ChannelDefaults
    Orderer:
      <<: *OrdererDefaults
      Organizations:
        - *OrdererOrg
      Capabilities: *OrdererCapabilities
    Application:
      <<: *ApplicationDefaults
      Organizations:<% for (let i = 0; i < peerOrgs.length; i++) { %>
        - *<%= peerOrgs[i].charAt(0).toUpperCase() + peerOrgs[i].slice(1)%><% } %>
    Capabilities: *ApplicationCapabilities
