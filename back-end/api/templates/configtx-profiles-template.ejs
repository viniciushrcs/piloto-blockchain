  <%= profileName %>:
    <<: *ChannelDefaults
    Orderer:
      <<: *OrdererDefaults
      Organizations:
        - *OrdererOrg
      Capabilities: *OrdererCapabilities
    Application:
      <<: *ApplicationDefaults
      Organizations:<% for (let i = 0; i < peerOrgs.length; i++) { %>
        - *Org<%= i %><% } %>
    Capabilities: *ApplicationCapabilities
